      ? [...selectedSubCats, value]\r\n        : selectedSubCats.filter((item) => item !== value)\r\n    );\r\n  };\r\n\r\n  console.log(selectedSubCats);\r\n\r\n  return (\r\n    <div className=\"flex mx-5 mt-8 gap-32\">\r\n      <div className=\"left h-full space-y-3 sticky top-8\">\r\n        <div className=\"filterItem space-y-2\">\r\n          <h2 className=\"text-xl\">Product Categories</h2>\r\n          {data?.map((item) => (\r\n            <div className=\"inputItem\" key={item.id}>\r\n              <input\r\n                type=\"checkbox\"\r\n                id={item.id}\r\n                value={item.id}\r\n                onChange={handleChange}\r\n              />\r\n              <label htmlFor=\"1\">{item.attributes.title}</label>\r\n            </div>\r\n          ))}\r\n        </div>\r\n        <div className=\"filterItem space-y-2\">\r\n          <h2 className=\"text-xl\">Filter by price</h2>\r\n          <div className=\"inputItem\">\r\n            <span>0</span>\r\n            <input\r\n              type=\"range\"\r\n              min={0}\r\n              max={1000}\r\n              onChange={(e) => setMaxPrice(e.target.value)}\r\n            />\r\n            <span>{maxPrice}</span>\r\n          </div>\r\n        </div>\r\n        <div className=\"filterItem space-y-2\">\r\n          <h2 className=\"text-xl\">Sort by</h2>\r\n          <div className=\"inputItem\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"asc\"\r\n              value=\"asc\"\r\n              name=\"price\"\r\n              onChange={(e) => setSort(\"asc\")}\r\n            />\r\n            <label htmlFor=\"asc\">Price (Lowest first)</label>\r\n          </div>\r\n          <div className=\"inputItem\">\r\n            <input\r\n              type=\"radio\"\r\n              id=\"desc\"\r\n              value=\"desc\"\r\n              name=\"price\"\r\n              onChange={(e) => setSort(\"desc\")}\r\n            />\r\n            <label htmlFor=\"desc\">Price (Highest first)</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div className=\"right space-y-12 flex-1\">\r\n        <div className=\"w-full\">\r\n          <img\r\n            src=\"https://images.pexels.com/photos/2704496/pexels-photo-2704496.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=2\"\r\n            alt=\"\"\r\n            className=\"h-64 object-cover\"\r\n          />\r\n        </div>\r\n        <List\r\n          catId={catId}\r\n          maxPrice={maxPrice}\r\n          sort={sort}\r\n          subCats={selectedSubCats}\r\n        />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Products;\r\n","C:\\Users\\Smert\\OneDrive\\Masaüstü\\coding\\React\\react_website_projects\\react_shopping_app\\client\\src\\pages\\Product\\Product.jsx",["210"],[],"import AddShoppingCartIcon from \"@mui/icons-material/AddShoppingCart\";\r\nimport FavoriteBorderIcon from \"@mui/icons-material/FavoriteBorder\";\r\nimport BalanceIcon from \"@mui/icons-material/Balance\";\r\nimport useFetch from \"../../hooks/useFetch\";\r\nimport { useState, useEffect } from \"react\";\r\nimport { useParams } from \"react-router-dom\";\r\nimport { useDispatch } from \"react-redux\";\r\nimport { addToCart } from \"../../redux/cartReducer\";\r\n\r\nconst Product = () => {\r\n  const id = useParams().id;\r\n  const dispatch = useDispatch();\r\n  const [quantity, setQuantity] = useState(1);\r\n  const { data, loading, error } = useFetch(`/products/${id}?populate=*`);\r\n\r\n  const imgUrl =\r\n    process.env.REACT_APP_UPLOAD_URL +\r\n    data?.attributes?.img?.data?.attributes?.url;\r\n  const img2Url =\r\n    process.env.REACT_APP_UPLOAD_URL +\r\n    data?.attributes?.img2?.data?.attributes?.url;\r\n\r\n  const [selectedImg, setSelectedImg] = useState(imgUrl);\r\n\r\n  useEffect(() => {\r\n    if (!loading && imgUrl) {\r\n      setSelectedImg(imgUrl);\r\n    }\r\n  }, [loading, imgUrl]);\r\n\r\n  const clickHandler = (url) => {\r\